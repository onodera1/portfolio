<div>相談ランキング</div>
<% @counselinglikes.each.with_index(1) do |counselinglike,i| %>
	<h5>第 <%= i %> 位</h5>
	<%=link_to counselinglike.title ,counseling_path(counselinglike.id)  %>
<% end %><br><br>

<div>解決ランキング</div>
<% @counselingcommentlikes.each.with_index(1) do |counselingcommentlike,i| %>
	<h5>第 <%= i %> 位</h5>
	<%=link_to counselingcommentlike.comment ,counseling_path(counselingcommentlike.id)  %>
<% end %><br><br>



<div>相談一覧</div>
<% @counselings.each do |counseling| %>
　　<p>タイトル</p>
   <span><%=link_to counseling.title ,counseling_path(counseling.id) %></span>
<% end %><br>
<%= paginate @counselings %>
<%= link_to '戻る', root_path %>
